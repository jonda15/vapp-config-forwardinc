<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:dxkn="http://www.ca.com/Directory/Configuration/Knowledge" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.ca.com/Directory/Configuration/Knowledge" elementFormDefault="qualified" attributeFormDefault="qualified" version="1.0">
   <xs:simpleType name="password">
      <xs:annotation><xs:appinfo><screenDirections secret="true"/></xs:appinfo></xs:annotation>
      <xs:restriction base="xs:string">
         <xs:whiteSpace value="preserve"/>
         <xs:minLength value="1"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="DistinguishedName">
      <xs:annotation>
         <xs:documentation>Need to make sure that xml sensitive characters are supported.

			Some possible references:

			xmlns:if="http://xmled.info/ns/X.500/4/InformationFramework" 

			see also http://www.ietf.org/internet-drafts/draft-legg-xed-protocols-03.txt
         </xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:whiteSpace value="preserve"/>
         <xs:pattern value="^((((\*|[A-Za-z][-A-Za-z0-9]*))|([0-9]+(\.[0-9]+)+))=([^,+]|(\\\\,)|(\\\\\+))*)((\+ *)(((\*|[A-Za-z][-A-Za-z0-9]*))|([0-9]+(\.[0-9]+)+))=([^,+]|(\\\\,)|(\\\\\+))*)*(, *((((\*|[A-Za-z][-A-Za-z0-9]*))|([0-9]+(\.[0-9]+)+))=([^,+]|(\\\\,)|(\\\\\+))*)(\+(((\*|[A-Za-z][-A-Za-z0-9]*))|([0-9]+(\.[0-9]+)+))=([^,+]|(\\\\,)|(\\\\\+))*)*)*$">
            <xs:annotation>
               <xs:appinfo><messageKeyBase value="dn"/></xs:appinfo>
            </xs:annotation>
         </xs:pattern>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="octetstring">
      <xs:restriction base="xs:string">
         <xs:whiteSpace value="preserve"/>
         <xs:minLength value="1"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="port">
      <xs:annotation>
         <xs:documentation>An IP Port used for network communications</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:int">
         <xs:minInclusive value="1"/>
         <xs:maxInclusive value="65535"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="pollInterval">
      <xs:annotation>
         <xs:documentation>Snmp poll interval</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:positiveInteger">
         <xs:minInclusive value="120"/>
         <xs:maxInclusive value="7200"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="tcpport">
